# edge.mt -- testing cutting edge features in mint-lang

extern printf
extern getnumargs

func bind(f, ...)
	return {
		bound = f,
		args = args
	}
end

func callr(b, ...)
	return b.bound(expand(b.args, len(b.args)), expand(args, getnumargs(b.bound) - len(b.args)))
end

func _main()
	
	return 0
end
